<Window x:Class="Unit_3_Final.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Unit_3_Final"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="LetterImput"></TextBox>
        <RadioButton x:Name="Easy">Easy Mode</RadioButton>
        <RadioButton x:Name="Medium">Medium Mode</RadioButton>
        <RadioButton x:Name="Hard">Hard Mode</RadioButton>
        <Button x:Name="BtnCheck" Click="BtnCheck_Click">Start Game</Button>
        <TextBlock x:Name="Output"></TextBlock>
    </StackPanel>
</Window>
